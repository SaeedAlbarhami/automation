    <script type="text/javascript">
        $(window).on("load", function () {
            ClickHyperLinkWhenPressEnterOnKeyboard('CUserName', 'btnsubmitCustomer');
            ClickHyperLinkWhenPressEnterOnKeyboard('CPassword', 'btnsubmitCustomer');

            ClickHyperLinkWhenPressEnterOnKeyboard('Password', 'btnsubmitContractor');
            ClickHyperLinkWhenPressEnterOnKeyboard('UserName', 'btnsubmitContractor');

            ClickHyperLinkWhenPressEnterOnKeyboard('Mobile', 'ContractorOtp');

            ClickHyperLinkWhenPressEnterOnKeyboard('OtpCode', 'btnsubmitContractorByotp');
            ClickHyperLinkWhenPressEnterOnKeyboard('UserNameOtp', 'btnsubmitContractorByotp');

            ClickHyperLinkWhenPressEnterOnKeyboard('COtpCode', 'btnsubmitCustomerByotp');

            ClickHyperLinkWhenPressEnterOnKeyboard('EmiratesId', 'OtpCustomer');
          
        });
        $(document).ready(function () {
        

            $("#btnsubmitContractor, #ContractorOtp, #btnsubmitContractorByotp").click(function (event) {
                $("#UserType").val(2);
                $('#invalidCustomerLoginMessage').html('').hide();
                $('#invalidCustomerLoginMessageDefault').html('').hide();
                $('#invalidLoginMessage').html('').hide();
                $('#invalidLoginMessageDefault').html('').hide();
                $("#OtpCustomerSuccessMessage").html("");
                $("#OtpSuccessMessage").html("");
                var isValid = true;
                var message = 'الحقل مطلوب';
                if (this.id == 'ContractorOtp') {

                    var un = $('#Mobile').val();
                    if (!un && un.trim().length <= 0) {
                        isValid = false;
                        $('#msg_Mobile').html(message).show();
                    } else {
                        $('#msg_Mobile').html('').hide();
                    }
                    var un = $('#UserNameOtp').val();
                    if (!un && un.trim().length <= 0) {
                        isValid = false;
                        $('#msg_UserNameOtp').html(message).show();
                    } else {
                        $('#msg_UserNameOtp').html('').hide();
                    }

                    if (isValid) {
                        $("#Password").val("");
                        angular.element(document.getElementById('loginForm')).scope().generateOtp('ar');
                    }
                }
                else {
                    if (this.id == 'btnsubmitContractorByotp') {
                        var un = $('#OtpCode').val();
                        if (!un && un.trim().length <= 0) {
                            isValid = false;
                            $('#msg_OtpCode').html(message).show();
                        } else if (isNaN(un)) {
                            isValid = false;
                            $('#msg_OtpCode').html('الرجاء تعبئة القيمة الصحيحة').show();
                        } else {
                            $('#msg_OtpCode').html('').hide();
                        }

                        var un = $('#UserNameOtp').val();
                        if (!un && un.trim().length <= 0) {
                            isValid = false;
                            $('#msg_UserNameOtp').html(message).show();
                        } else {
                            $('#msg_UserNameOtp').html('').hide();
                        }
                        if (isValid) {
                            angular.element(document.getElementById('loginForm')).scope().contractorModel.Password = "";
                            angular.element(document.getElementById('loginForm')).scope().ContractorLogin(angular.element(document.getElementById('loginForm')).scope().contractorModel, 'ar');
                        }
                    }
                    if (this.id == 'btnsubmitContractor') {
                        var un = $('#UserName').val();
                        if (!un && un.trim().length <= 0) {
                            isValid = false;
                            $('#msg_UserName').html(message).show();
                        } else {
                            $('#msg_UserName').html('').hide();
                        }
                        var un = $('#Password').val();
                        if (!un && un.trim().length <= 0) {
                            isValid = false;
                            $('#msg_Password').html(message).show();
                        } else {
                            $('#msg_Password').html('').hide();
                        }
                       if (isValid)
                        angular.element(document.getElementById('loginForm')).scope().ContractorLogin(angular.element(document.getElementById('loginForm')).scope().contractorModel,'ar');
                    }

                }
                    if (!isValid)
                        event.preventDefault();

            });
            $("#btnsubmitCustomer, #btnsubmitCustomerByotp, #OtpCustomer").click(function (event) {
                $("#UserType").val(1);
                $('#invalidCustomerLoginMessage').html('').hide();
                $('#invalidCustomerLoginMessageDefault').html('').hide();
                $('#invalidLoginMessage').html('').hide();
                $('#invalidLoginMessageDefault').html('').hide();
                $("#OtpCustomerSuccessMessage").html("");
                $("#OtpSuccessMessage").html("");
                var isValid = true;
                 var message = 'الحقل مطلوب';
                if (this.id == 'btnsubmitCustomer') {
                    var un = $('#CUserName').val();
                    if (!un && un.trim().length <= 0) {
                        isValid = false;
                        $('#msg_CUserName').html(message).show();
                    } else {
                        $('#msg_CUserName').html('').hide();
                    }
                    var un = $('#CPassword').val();
                    if (!un && un.trim().length <= 0) {
                        isValid = false;
                        $('#msg_CPassword').html(message).show();
                    } else {
                        $('#msg_CPassword').html('').hide();
                    }
                     if (isValid)
                         angular.element(document.getElementById('loginForm')).scope().CustomerLogin(angular.element(document.getElementById('loginForm')).scope().customerModel,'ar');
                }

                if (this.id == 'btnsubmitCustomerByotp') {
                    var un = $('#COtpCode').val();
                    if (!un && un.trim().length <= 0) {
                        isValid = false;
                        $('#msg_COtpCode').html(message).show();
                        $("#OtpCustomerSuccessMessage").html("");
                    } else if (isNaN(un)) {
                        isValid = false;
                        $('#msg_COtpCode').html('الرجاء تعبئة القيمة الصحيحة').show();
                        $("#OtpCustomerSuccessMessage").html("");
                    } else {
                        $('#msg_COtpCode').html('').hide();
                    }
                    $("#CUserName").val("");
                    $("#CPassword").val("");
                    angular.element(document.getElementById('loginForm')).scope().customerModel.UserName = "";
                    angular.element(document.getElementById('loginForm')).scope().customerModel.Password = "";
                     if (isValid)
                         angular.element(document.getElementById('loginForm')).scope().CustomerLogin(angular.element(document.getElementById('loginForm')).scope().customerModel,'ar');

                }
                if (this.id == 'OtpCustomer') {
                    var un = $('#EmiratesId').val();
                    if (!un && un.trim().length <= 0 ) {
                        isValid = false;
                        $('#msg_EmiratesID').html(message).show();
                    } else if (!IsEmiratesIdValid(un) && un !="123197012345671") {
                        isValid = false;
                        $('#msg_EmiratesID').html('رقم الهوية الإماراتية غير صحيح').show();
                    } else {
                        $('#msg_EmiratesID').html('').hide();
                    }

                if (!isValid)
                    event.preventDefault();
                else
                {
                    $("#CUserName").val("");
                    $("#CPassword").val("");
                    angular.element(document.getElementById('loginForm')).scope().customerModel.UserName = "";
                    angular.element(document.getElementById('loginForm')).scope().customerModel.Password = "";
                    angular.element(document.getElementById('loginForm')).scope().generateOtpCustomer('ar');
                }
                }
                 if (!isValid)
                    event.preventDefault();


            });

        });


    </script>



                        </div>
                    </div>
                </div>
            </div>
       

